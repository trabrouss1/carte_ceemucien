{% extends 'base.html.twig' %}

{% block title %}Inscription carte de membre CEEMUCI{% endblock %}

{% block body %}
  

    <section class="contact-us section breadcrumbs overlay">
        <div class="container">
            <div class="inner">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="contact-us-left">
                           <img src="carteMembre.jpg"/>
                        </div>
                    </div>
                    <div class="col-lg-8">
                        <div class="contact-us-form">
                            <h2>Votre carte de membre CEEMUCI ici!!!</h2>
                            <p>Si vous avez des questions, n'hésitez pas à nous contacter</p>
                            <!-- Form -->
                            <form class="form" method="POST" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="file" accept="image/*" name="photo" placeholder="Photo" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" name="nom" placeholder="Nom" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" name="prenom" placeholder="Prénom" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" name="contact" placeholder="Téléphone" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <input type="text" name="contactCasUrgent" placeholder="Téléphone cas urgent" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <select name="genre" required>
                                                <option>M</option>
                                                <option>F</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-6">
                                        <div class="form-group">
                                            <select name="coordination" required>
                                                <option>UFHB</option>
                                                <option>LOH</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="text" name="qualite" placeholder="Qualité" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="text" name="niveau" placeholder="Niveau d'étude ou votre profession" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="text" name="matricule" placeholder="Matricule" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="date" name="dateNaissance" placeholder="Lieu de naissance" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="text" name="lieuNaissance" placeholder="Lieu de naissance" required>
                                        </div>
                                    </div>
                                    <div class="col-lg-12">
                                        <div class="form-group">
                                            <input type="text" name="villeActuelle" placeholder="Ville de residence" required>
                                        </div>
                                    </div>
                                    <div class="col-12">
                                        <div class="form-group login-btn">
                                            <button class="btn" type="submit">Envoyer</button>
                                        </div>
{#                                        <div class="checkbox">#}
{#                                            <label class="checkbox-inline" for="2"><input name="news" id="2" type="checkbox">Do you want to subscribe our Newsletter ?</label>#}
{#                                        </div>#}
                                    </div>
                                </div>
                            </form>
                            <!--/ End Form -->
                        </div>
                    </div>
                </div>
            </div>
            <div class="contact-info">
                <div class="row">
                    <!-- single-info -->
                    <div class="col-lg-4 col-12 ">
                        <div class="single-info">
                            <i class="icofont icofont-ui-call"></i>
                            <div class="content">
                                <h3>+(225) 07 59 637 013</h3>
                                <p>benceemuci@gmail.com</p>
                            </div>
                        </div>
                    </div>
                    <!--/End single-info -->
                    <!-- single-info -->
                    <div class="col-lg-4 col-12 ">
                        <div class="single-info">
                            <i class="icofont-google-map"></i>
                            <div class="content">
                                <h3>Abidjan Côte d'Ivoire</h3>
                                <p>Cocody, Rivera palmeraie</p>
                            </div>
                        </div>
                    </div>
                    <!--/End single-info -->
                    <!-- single-info -->
                    <div class="col-lg-4 col-12 ">
                        <div class="single-info">
                            <i class="icofont icofont-wall-clock"></i>
                            <div class="content">
                                <h3>24/24</h3>
                                <p>Ouvert tous les jours</p>
                            </div>
                        </div>
                    </div>
                    <!--/End single-info -->
                </div>
            </div>
        </div>
    </section>
{% endblock %}
